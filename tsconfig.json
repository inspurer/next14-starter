{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "paths": {
      "@/*": ["./src/*"]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/(auth)/login/page.jsx", "src/app/contact/page.jsx", "src/app/admin/page.jsx", "src/app/(auth)/forgot-password/page.jsx", "src/app/(auth)/register/page.jsx", "src/app/about/page.jsx", "src/app/blog/page.jsx", "src/app/blog/[slug]/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
